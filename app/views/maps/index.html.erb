<h1>Listing maps</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Lat</th>
    <th>Lng</th>
    <th>Zoom</th>
  </tr>

<% for map in @maps %>
  <tr>
    <td><%=h map.name %></td>
    <td><%=h map.description %></td>
    <td><%=h map.lat %></td>
    <td><%=h map.lng %></td>
    <td><%=h map.zoom %></td>
    <td><%= link_to 'Show', map %></td>
    <td><%= link_to 'Edit', edit_map_path(map) %></td>
    <td><%= link_to 'Destroy', map, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New map', new_map_path %>
